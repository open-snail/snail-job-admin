    "@vitejs/plugin-vue": "5.0.5",
    "@vitejs/plugin-vue-jsx": "4.0.0",
    "eslint": "9.7.0",
    "eslint-plugin-vue": "9.27.0",
    "lint-staged": "15.2.7",
    "sass": "1.77.8",
    "simple-git-hooks": "2.11.1",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "unplugin-icons": "0.19.0",
    "unplugin-vue-components": "0.27.3",
    "vite": "5.3.4",
    "vite-plugin-progress": "0.0.7",
