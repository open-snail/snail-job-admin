    "vue-draggable-plus": "0.5.2",
    "vue-i18n": "9.13.1",
    "vue-router": "4.4.0",
    "vue3-puzzle-vcode": "^1.1.7"
  },
  "devDependencies": {
    "@elegant-router/vue": "0.3.7",
