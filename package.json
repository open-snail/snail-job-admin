    "@sa/scripts": "workspace:*",
    "@sa/uno-preset": "workspace:*",
    "@soybeanjs/eslint-config": "1.3.7",
    "@types/node": "20.14.11",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "0.61.5",
    "@unocss/preset-icons": "0.61.5",
    "@unocss/preset-uno": "0.61.5",
