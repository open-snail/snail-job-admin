    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "@vueuse/core": "10.11.0",
    "clipboard": "2.0.11",
    "dayjs": "1.11.12",
    "echarts": "5.5.1",
    "highlight.js": "^11.10.0",
    "naive-ui": "2.39.0",
    "nprogress": "0.2.0",
